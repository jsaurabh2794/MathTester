<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="false" >
        <shape >
            <solid
                android:color="#ef4483" />
            <stroke
                android:width="1dp"
                android:color="#121111" />
            <corners
                android:radius="15dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>

    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#3a3c71" />
            <stroke
                android:width="1dp"
                android:color="#121111" />
            <corners
                android:radius="15dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>





</selector>